            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.lang.Long;

long timeStamp = System.currentTimeMillis();

log.info(vars.get(&quot;initialTime&quot;));

if(vars.get(&quot;initialTime&quot;).size() == 0){
	vars.put(&quot;initialTime&quot;,System.currentTimeMillis().toString());
}

vars.put(&quot;lastTime&quot;, System.currentTimeMillis().toString());

long init = vars.get(&quot;initialTime&quot;).toLong();
long last = vars.get(&quot;lastTime&quot;).toLong();

log.info((last - init).toString());

vars.put(&quot;differenceBetweenTimes&quot;, ((last - init)/1000).toString());

boolean contTest = ((last - init)/1000) &lt; 5400;

log.info(contTest.toString());

vars.put(&quot;continueTest&quot;, contTest.toString());
</stringProp>
